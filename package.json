{
    "name": "@interverse/three-scatter",
    "version": "1.0.0",
    "description": "High-performance instanced scatter systems for Three.js with chunk-based LOD",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc && tsc -p tsconfig.esm.json",
        "build:cjs": "tsc",
        "build:esm": "tsc -p tsconfig.esm.json",
        "clean": "rimraf dist",
        "prepublishOnly": "yarn clean && yarn build"
    },
    "keywords": [
        "three",
        "threejs",
        "three.js",
        "scatter",
        "instancing",
        "instancedmesh",
        "vegetation",
        "grass",
        "foliage",
        "procedural",
        "chunks",
        "lod"
    ],
    "author": "Interverse Engine",
    "license": "MIT",
    "peerDependencies": {
        "three": ">=0.181.0"
    },
    "devDependencies": {
        "@types/three": "^0.181.0",
        "rimraf": "^5.0.0",
        "typescript": "^5.3.0"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "bugs": {
        "url": ""
    },
    "homepage": ""
}